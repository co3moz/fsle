{
  "manifest_version": 2,
  "name": "Facebook scroll limiter",
  "description": "Facebook scroll down limit for safety reasons",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["fsle.js"]
    }
  ],
  "permissions": [
    "webRequest",
    "*://*.facebook.com/*",
    "webRequestBlocking"
  ]
}